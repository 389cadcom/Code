  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 9222,
      "webRoot": "${workspaceFolder}/src"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "启动 Chrome 并打开 localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/src"
    }
  ]
}

